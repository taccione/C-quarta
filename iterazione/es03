/* Dato N un numero intero positivo, generare e visualizzare in ordine crescente i numeri dispari
minori o uguali a N.
*/
#include <stdio.h>
int main(int argc, char *argv[])
{
    // Dichiarazione delle variabili
    int n;

    do
    {
        printf("Inserisci un numero intero positivo: ");
        scanf("%d", &n);
    } while (n < 0);

    for (int i = 1; i <= n; i++)
    {
        if (i % 2 != 0) {
            printf("%d ", i);
        }
    }

    return 0;
}
