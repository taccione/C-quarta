/* Dati due numeri interi positivi N1 e N2 con N2>N1, generare e
visualizzare in ordine crescente i numeri interi compresi
nell'intervallo chiuso [N1,N2].
*/
#include <stdio.h>
int main(int argc, char *argv[])
{
    // Dichiarazione delle variabili
    int n1, n2;

    do
    {
        printf("Inserisi il primo numero:  ");
        scanf("%d", &n1);
        printf("Inserisci il secondo numero: ");
        scanf("%d", &n2);

    } while (n1 > n2);

    int numDaGenerare = n1;

    while (numDaGenerare < n2)
    {
        numDaGenerare++;
        printf("%d ", numDaGenerare);
    }

return 0;
}
